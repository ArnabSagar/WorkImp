{
  "manifest_version": 2,
  "name": "WorkImp",
  "description": "Fetches current tab url.",
  "version": "1.0",
  "author": "Arnab Sagar",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "tracking current URL"
  },
  "permissions": ["tabs", "activeTab", "http://127.0.0.1:5000/*", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
