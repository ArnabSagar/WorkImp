{
  "manifest_version": 2,
  "name": "WorkImp",
  "description": "Fetches current tab url.",
  "version": "1.0",
  "author": "Arnab Sagar",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "WorkImp Settings"
  },
  "background": {
    "scripts": ["background.js", "searchWhitelist.js"],
    "persistent": false
  }
}
