{
  "manifest_version": 2,
  "name": "WorkImp",
  "description": "Fetches current tab url.",
  "version": "1.0",
  "author": "Arnab Sagar and Your Boi Nathan",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "tracking current URL"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "contentscripts": {
    "js": ["script.js"]
    "persistent": false
  }
}
